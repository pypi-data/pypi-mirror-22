IMAGE_NAME={{ image_name }}
singularity exec  \
  -B {{ dataset_path }}:/input_dataset  \
  -B {{ output_root }}:/output  \
  -c \
  $IMAGE_NAME  \
  _analyse_by_ids  \
    --cwl_tool_wrapper_path {{ cwl_tool_wrapper_path }}  \
    --input_dataset_path=/input_dataset  \
    --output_root=/output  \
    {{ hash_ids -}}
