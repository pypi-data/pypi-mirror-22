{
    "$schema": "http://json-schema.org/schema#",
    "additionalProperties": false,
    "properties": {
        "$schema": {
            "format": "url",
            "type": "string"
        },
        "_collections": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "_private_notes": {
            "items": {
                "$ref": "elements/sourced_value.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "acronym": {
            "items": {
                "type": "string"
            },
            "title": "Conference acronym",
            "type": "array"
        },
        "address": {
            "items": {
                "$ref": "elements/address.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "alternative_titles": {
            "items": {
                "$ref": "elements/title.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "closing_date": {
            "format": "date",
            "title": "Conference closing date",
            "type": "string"
        },
        "cnum": {
            "$ref": "elements/cnum.json"
        },
        "contact_details": {
            "items": {
                "$ref": "elements/contact.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "control_number": {
            "type": "integer"
        },
        "deleted": {
            "type": "boolean"
        },
        "deleted_records": {
            "description": "List of deleted records referring to this record",
            "items": {
                "$ref": "elements/json_reference.json"
            },
            "title": "Deleted Records",
            "type": "array"
        },
        "external_system_identifiers": {
            "items": {
                "anyOf": [
                    {
                        "additionalProperties": false,
                        "properties": {
                            "schema": {
                                "enum": [
                                    "SPIRES"
                                ],
                                "type": "string"
                            },
                            "value": {
                                "pattern": "^CONF-\\d+$",
                                "type": "string"
                            }
                        },
                        "required": [
                            "schema",
                            "value"
                        ],
                        "type": "object"
                    }
                ]
            },
            "type": "array",
            "uniqueItems": true
        },
        "inspire_categories": {
            "items": {
                "$ref": "elements/inspire_field.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "keywords": {
            "items": {
                "additionalProperties": false,
                "properties": {
                    "source": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                },
                "required": [
                    "value"
                ],
                "type": "object"
            },
            "title": "Keywords",
            "type": "array",
            "uniqueItems": true
        },
        "legacy_creation_date": {
            "format": "date-time",
            "type": "string"
        },
        "new_record": {
            "$ref": "elements/json_reference.json",
            "description": "Master record that replaces this record",
            "title": "New record"
        },
        "opening_date": {
            "format": "date",
            "title": "Conference opening date",
            "type": "string"
        },
        "place": {
            "pattern": "^.+,.+,?.*$",
            "title": "Conference place",
            "type": "string"
        },
        "public_notes": {
            "items": {
                "$ref": "elements/sourced_value.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "self": {
            "$ref": "elements/json_reference.json",
            "description": "Url of the record itself",
            "title": "Url of the record"
        },
        "series": {
            "items": {
                "additionalProperties": false,
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "number": {
                        "type": "integer"
                    }
                },
                "required": [
                    "name"
                ],
                "type": "object"
            },
            "title": "Conference series to which a conference belongs.",
            "type": "array",
            "uniqueItems": true
        },
        "short_description": {
            "items": {
                "$ref": "elements/sourced_value.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "titles": {
            "items": {
                "$ref": "elements/title.json"
            },
            "type": "array",
            "uniqueItems": true
        },
        "urls": {
            "items": {
                "$ref": "elements/url.json"
            },
            "type": "array",
            "uniqueItems": true
        }
    },
    "title": "Conference",
    "type": "object"
}
