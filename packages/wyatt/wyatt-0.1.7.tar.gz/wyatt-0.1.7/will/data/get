
import will
import argparse
parser = argparse.ArgumentParser(description="Get william's messages.")
parser.add_argument('--quantity', '-q', '-n', metavar='quantity', type=int, nargs='?', help="The number of messages you want to get.", default=5)
parser.add_argument('--offset', '-o', metavar='offset', type=int, nargs='?', help="How far back in messages do you want to see?", default=0)
args=parser.parse_args()
will.display(will.get(quantity=args.quantity, offset=args.offset))