#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'bodhi-client==2.7.0','console_scripts','bodhi'
__requires__ = 'bodhi-client==2.7.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('bodhi-client==2.7.0', 'console_scripts', 'bodhi')()
    )
