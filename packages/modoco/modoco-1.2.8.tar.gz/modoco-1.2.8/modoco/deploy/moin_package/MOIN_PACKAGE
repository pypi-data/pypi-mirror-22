InstallPackage|LanguageSetup|English--all_system_pages.zip
InstallPackage|LanguageSetup|English--all_help_pages.zip
# modoco custom plugins
InstallPlugin|History.py|local|macro|History.py
InstallPlugin|PublishPage.py|local|action|PublishPage.py
# overrides core newaccount.py to require superuser
InstallPlugin|newaccount.py|local|action|newaccount.py
# simpler to tweak one theme than make a whole new one
